{
  "name": "spotinfo",
  "version": "1.1",
  "description": "A simple extension for analysis of Spotify user accounts library and listening habits.",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "spotifo",
      "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
},
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
